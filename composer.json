{
    "name": "edwrodrig/image",
    "description": "Library optimize images for web sites and create thumbnails",
    "authors": [
        {
            "name": "Edwin Rodríguez León",
            "email": "edwrodrig@gmail.com",
            "homepage": "http://www.edwin.cl",
            "role": "Developer"
        }
    ],
    "keywords" : [
        "optimize-images",
        "compare-images",
        "thumbnails",
        "svg"
    ],
    "license" : "MIT",
    "minimum-stability" : "stable",
    "require": {
        "php" : "^7.4",
        "ext-imagick": "*",
        "ext-fileinfo": "*"
    },
    "require-dev" : {
        "phpunit/phpunit" : "^8.5",
        "mikey179/vfsstream": "^1.6"
    },
    "autoload" : {
        "psr-4" : {
            "edwrodrig\\image\\" : "src/"
        },
        "exclude-from-classmap": ["src/test/"]
    },
    "autoload-dev" : {
        "psr-4" : {
            "edwrodrig\\image\\test" : "src/test/"
        }
    },
    "archive" : {
        "exclude" : [
            "/examples",
            "/tests",
            "/.idea",
            "/.travis.yml",
            ".gitignore",
            ".git",
            "phpunit.xml.dist",
            "README.md"
        ]
    }
}
