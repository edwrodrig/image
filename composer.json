{
    "name": "edwrodrig/image",
    "description": "Library optimize images for web sites and create thumbnails",
    "authors": [
        {
            "name": "Edwin Rodríguez León",
            "email": "edwrodrig@gmail.com",
            "homepage": "http://www.edwin.cl",
            "role": "Developer"
        }
    ],
    "keywords" : [
        "optimize-images",
        "compare-images",
        "thumbnails",
        "svg"
    ],
    "license" : "MIT",
    "minimum-stability" : "stable",
    "require": {
        "php" : "^7.2",
        "ext-imagick": "*"
    },
    "require-dev" : {
        "phpunit/phpunit" : "^7.0"
    },
    "autoload" : {
        "psr-4" : {
            "edwrodrig\\image\\" : "src/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "tests\\edwrodrig\\image\\" : "src/"
        }
    },
    "archive" : {
        "exclude" : [
            "/examples",
            "/tests",
            "/.idea",
            ".gitignore",
            ".git",
            "phpunit.xml.dist",
            "README.md"
        ]
    }
}
